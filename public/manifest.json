{
	"name": "Vue TranZact - Tally Integration",
	"short_name": "TranZact",
	"manifest_version": 3,
	"description": "Tool for syncing TranZact data into your Tally Software",
	"version": "1.0.0.0",
	"action": {
		"default_title": "Tranzact!",
		"default_icon": "img/tranzact.png"
	},
	"icons": {
		"16": "img/tranzact_16.png",
		"48": "img/tranzact_48.png",
		"128": "img/tranzact_128.png"
	},
	"background": {
		"service_worker": "system_files/background.js"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"host_permissions": [
		"http://localhost/*",
		"https://app.letstranzact.com/*",
		"https://maps.googleapis.com/maps/api/*"
	],
	"permissions": [
		"storage"
	],
	"web_accessible_resources": [
		{
			"resources": ["img/*", "fonts/*", "css/*", "js/*"],
			"matches": ["<all_urls>"]
		}
	],
	"google_api_key": "AIzaSyA4nfXyzwqGxzdzfmN5pbdVQVetNswKLn4"
}
